/* pages/owner/owner.wxss */
.UCenter-bg {
 /*  background-image: url(https://image.weilanwl.com/color2.0/index.jpg); 
  background-size: cover; */
  background-color: #f0145a;
  height: 350rpx;
  display: flex;
  justify-content: center;
  padding-top: 20rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 200rpx;
  height: 200rpx;
}

.UCenter-bg .gif-wave{
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  width: 750rpx;
  height: 300rpx;
}
.user-icon {
  display: block;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50% 50%;
  margin: 0 auto;
  border: solid 5px rgba(255, 255, 255, 0.5);
}
.contact-btn {
  display: inline-block;
  position: absolute;
  width: 100%;
  top: 0;
  left: 0;
  background: salmon;
  opacity: 0;
}
.cu-custom .cu-bar {
  padding-right: inherit;
  background-image: none;
}