.canui-address{
    padding:30rpx 20rpx;
}
.canui-address .cu-list{
    position: relative;
}
.canui-address .cu-list.menu>.cu-item{
    padding: 30rpx;
}
.canui-address .cu-list .cu-item .content view text{
    margin-left: 20rpx;
}

.canui-card-order{
    padding:0 20rpx;
}
.canui-card-order .canui-order-item{
    position: relative;
    margin-bottom:30rpx;
    padding:20rpx;
    padding-bottom:0rpx;
}
.canui-card-fiex-radius-box{
    width:100%;
    z-index:9;
}
.canui-card-fiex-radius-box .cu-list{
    border-radius:18rpx;
    box-shadow:0 3rpx 6rpx rgba(0, 0, 0, 0.1);
}
.canui-card-fiex-radius-box .cu-list.menu>.cu-item{
    padding:30rpx;
}
.head-title-box .icon-shop{
    margin-right:10rpx;
}
.head-title-box .icon-right{
    margin-left:5rpx;
}
.canui-order-item .cu-list.menu-avatar>.cu-item{
    padding:0;
    padding-left:110rpx;
}
.canui-order-item .cu-list.menu-avatar>.cu-item>.cu-avatar{
    left:0rpx;
}
.canui-order-item .cu-list .cu-item .canui-xzwz{
    -webkit-line-clamp:2;
}
.canui-order-item .cu-list.menu>.cu-item .content{
    font-size:27rpx;
    margin-right:40rpx;
}
.canui-order-item .cu-list.menu-avatar>.cu-item .action {
    text-align:right;
    margin-top:-10rpx;
    position: absolute;
    right: 0;
}
.canui-order-item .cu-list.menu>.cu-item .action .text-price{
    font-size:28rpx;
}
.canui-order-item .order-price-box{
    font-size:26rpx;
}
.cu-list.menu>.cu-item,.canui-input{
    min-height: 80rpx;
}
.canui-input .title,.canui-input input,.cu-list.menu>.cu-item .content{
    font-size:27rpx;
}
.cu-list.menu>.cu-item.arrow{
    padding-right:60rpx;
}

.canui-coupon{
    padding:0 20rpx;
}

.order-price-log{
    padding:0 20rpx;
    margin-top:30rpx;
}
.order-price-log .log-card{
    padding:20rpx 30rpx;
}
.order-price-log .log-list{
    padding:5rpx 0;
    height: 60rpx;
}
.order-price-log-zj{
    padding:20rpx 30rpx;
    background:#fafafa;
}
.canui-submit-view{
    z-index:99999;
}
.canui-pay-tip{
    padding:10rpx 20rpx;
}
.canui-submit{
    height:80rpx;
    padding-left:20rpx;
}
.canui-submit .canui-price{
    line-height:80rpx;
    font-size:40rpx;
}
.canui-submit .canui-submit-btn{
    float:right;
    height:80rpx;
    line-height:80rpx;
    width:180rpx;
    text-align:center;
    font-size:26rpx;
}
.stepper {  
    width:90px;  
    height: 26px;  
    /*给主容器设一个边框*/  
    border: 1rpx solid #818284;  
    border-radius: 3px;  
    margin:0 auto;  
}  
  
/*加号和减号*/  
.stepper .sign {  
    width: 24px;  
    line-height: 26px;  
    text-align: center;  
    float: left;  
}  
  
/*数值*/  
.stepper .number {  
    width: 40px;  
    height: 26px;  
    float: left;  
     margin: 0 auto;   
    text-align: center;  
    font-size: 12px;  
    color: #000000;
    /*给中间的input设置左右边框即可*/  
    border-left: 1rpx solid #818284;  
    border-right: 1rpx solid #818284;  
    
}  
  
/*普通样式*/  
.stepper .normal{  
    color: black;  
}  
  
/*禁用样式*/  
.stepper .disabled{  
    color: #ccc;  
}  

